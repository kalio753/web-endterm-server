{
    "name": "web-enterm-server",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "client": "cd client && yarn start",
        "server": "nodemon index.js",
        "dev": "concurrently --kill-others-on-fail \"yarn server\" \"yarn client\"",
        "start": "sudo node index.js",
        "build:client": "cd client && sudo npm run build"
    },
    "devDependencies": {
        "axios": "^1.3.4",
        "concurrently": "^7.6.0",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "nodemon": "^2.0.21"
    }
}
